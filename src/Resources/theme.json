{
  "name":"Creative Cave Modern, Creative, Responsive Theme by TheCodeBunny",
  "author":"TheCodeBunny",
  "views":[
    "@Storefront",
    "@Plugins",
    "@TcbCreativeCave"
  ],
  "style":[
    "app/storefront/src/scss/overrides.scss",
    "@Storefront",
    "app/storefront/src/scss/base.scss"
  ],
  "script":[
    "@Storefront",
    "app/storefront/dist/storefront/js/tcb-creative-cave.js"
  ],
  "asset":[
    "@Storefront",
    "app/storefront/src/assets"
  ],
  "configInheritance":[
    "@Storefront",
    "@OtherTheme"
  ],
  "config":{

    "blocks":{
      "tcb-google-settings":{
        "label":{
          "en-GB":"Google Related Settings",
          "de-DE":"Google Einstellungen"
        }
      }
    },

    "fields":{

       "sw-color-brand-primary":{
        "value":"#00ff00"
      },


      "tcb-google-fonts": {
        "label": {
          "de-DE": "Google Font einbinden",
          "en-GB": "Add google font"
        },
        "type": "switch",
        "value": true,
        "editable": true,
        "block": "tcb-google-settings",
        "helpText": {
          "en-GB": "Activate or deactivate google fonts. When you use Google fonts, you have to update your privacy policy.",
          "de-DE": "Aktivieren oder deaktivieren Sie die Google Fonts. Des Weiteren beachten Sie bitte, dass Sie Ihre Datenschutzerklärung durch das Nutzen der Google Font aktualisieren müssen."
        }
      }
    }
  }
}